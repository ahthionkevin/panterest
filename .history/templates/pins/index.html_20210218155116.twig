{% extends 'base.html.twig' %}

{% block title %}Find All Pins Here!{% endblock %}

{% block body %}
    <h1>{{ pluralize(pins|length,'Pin','Pins') }}</h1>

    {% for pin in pins %}
        <article>
            <h2><a href="#">{{ pin.getTitle() }}</a></h2>
            <p>{{ pin.getDescription() }}</p>
        </article>
    {% endfor %}
{% endblock %}
